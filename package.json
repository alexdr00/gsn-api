{
  "name": "gsn-api",
  "version": "0.0.1",
  "description": "Game Sale Notifier",
  "main": "index.ts",
  "scripts": {
    "ts-node": "ts-node --project tsconfig.dev.json",
    "ut": "jest",
    "ut:watch": "jest --watch",
    "tsc": "tsc -p tsconfig.prod.json",
    "start": "npm run tsc && node ./dist/index.js",
    "dev": "ts-node-dev --project tsconfig.dev.json --inspect=0.0.0.0:9229 -- ./index.ts --respawn",
    "migrate:schema": "npm run ts-node migrations/runners/schema.ts",
    "migrate:seed": "npm run ts-node migrations/runners/seed.ts",
    "migrate:trigger": "npm run ts-node migrations/runners/trigger.ts",
    "migrate:all": "npm-run-all -s migrate:trigger migrate:schema migrate:seed",
    "container-migrate:schema": "docker exec -it gsn-api npm run migrate:schema",
    "container-migrate:seed": "docker exec -it gsn-api npm run migrate:seed",
    "container-migrate:trigger": "docker exec -it gsn-api npm run migrate:trigger",
    "container-migrate:all": "docker exec -it gsn-api npm run migrate:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexdr00/gsn-api.git"
  },
  "author": "Alexander Dominguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdr00/gsn-api/issues"
  },
  "homepage": "https://github.com/alexdr00/gsn-api#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/cheerio": "^0.22.22",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.3",
    "@types/jsdom": "^16.2.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node-fetch": "^2.5.7",
    "@types/redis": "^2.8.25",
    "@types/string-similarity": "^3.0.0",
    "@types/uuid": "^8.0.0",
    "@types/verror": "^1.10.4",
    "amazon-cognito-identity-js": "^4.3.2",
    "aws-sdk": "^2.691.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsdom": "^16.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.1",
    "puppeteer": "^5.2.1",
    "redis": "^3.0.2",
    "string-similarity": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "uuid": "^8.1.0",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@types/pg": "^7.14.3",
    "@types/puppeteer": "^3.0.1",
    "@types/random": "^2.1.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.0.1",
    "node-mocks-http": "^1.8.1",
    "random": "^2.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0"
  }
}
